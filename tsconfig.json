{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@/*": ["./*"]
		},
		"jsx": "react-native",
		"module": "esnext",
		"moduleResolution": "bundler",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true,
		"target": "esnext",
		"lib": ["esnext", "dom", "dom.iterable"],
		"allowJs": true,
		"noEmit": true,
		"isolatedModules": true
	},
	"extends": "./node_modules/expo/tsconfig.base"
}
