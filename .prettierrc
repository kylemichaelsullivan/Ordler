{
	"semi": true,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"useTabs": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"printWidth": 100,
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"endOfLine": "auto",
	"plugins": ["prettier-plugin-tailwindcss"],
	"tailwindConfig": "./tailwind.config.js",
	"tailwindFunctions": ["clsx", "cn", "tw"],
	"singleAttributePerLine": true
}
